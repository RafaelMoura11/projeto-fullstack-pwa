generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Gender {
  MALE
  FEMALE
  OTHERS
}

model User {
  id          Int      @id @default(autoincrement())
  name        String
  birth_date  DateTime
  gender      Gender
  phone       String
  nationality String
}
